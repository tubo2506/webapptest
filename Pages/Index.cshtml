@page
@{
    ViewData["Title"] = "Home page";
}
<!DOCTYPE html>
<html>
<head>
    <title>My Website</title>
    <style>
        /* Add some style to the page */
        body {
            font-family: Arial, sans-serif;
            background-color: #effffe;
        }
        h1 {
            text-align: center;
            color: black;
            margin: auto;
        }
        nav {
            display: flex;
            justify-content: space-between;
            align-items: center;
            background-color: #00a89e;
        }
        nav ul {
            list-style: none;
            display: flex;
        }
        nav ul li {
            margin: 5px;
        }
        nav ul li a {
            text-decoration: none;
            font-size: 24px;
            font-weight: bold;
            color: white;
            position: relative;
            padding: 10px;
            border-radius: 10px;
            transition: 0.3s; /* Add transition for hover effects */
        }
        nav ul li a:hover {
            background-color: white; /* Change the background color on hover */
            color: darkblue; /* Change the text color on hover */
        }
        nav ul li a::before {
            content: url("icon.png"); /* Insert an icon from an image URL */
            margin-right: 10px; /* Add some space between the icon and the text */
        }
        .container {
            width: 80%;
            margin: 0 auto;
            display: flex;
            flex-wrap: wrap;
        }
        .card {
            width: 25%;
            margin: 10px;
            padding: 10px;
            border: 1px solid black;
            border-radius: 2%;
            box-shadow: 2px 2px 5px gray;
            background-color: white;
        }
        .card div:hover {
            box-shadow: 4px 4px 10px rgb(120, 118, 118);
        }
        .card img {
            width: 100%;
            height: 200px;
            object-fit: cover;
        }
        .card h3 {
            text-align: center;
        }
        .card p {
            text-align: justify;
        }
        .card a {
            display: block;
            text-align: center;
            text-decoration: none;
            color: blue;
        }
    </style>
</head>
